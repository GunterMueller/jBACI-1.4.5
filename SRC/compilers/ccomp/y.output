Grammar

    0 $accept: program $end

    1 program: init_outerscope outer_decls opt_main_decl
    2        | init_outerscope opt_main_decl

    3 init_outerscope: %empty

    4 outer_decls: outer_decl
    5            | outer_decls outer_decl

    6 outer_decl: include_decl
    7           | function_decl
    8           | monitor_decl
    9           | type_decl
   10           | const_decl
   11           | var_decl
   12           | extern_decl

   13 include_decl: start_include FNSTRING

   14 start_include: '#' INCLUDE

   15 opt_main_decl: %empty
   16              | main_hdr compound_stmt

   17 main_hdr: opt_main_type MAIN '(' ')'

   18 opt_main_type: %empty
   19              | INT
   20              | VOID

   21 compound_stmt: '{' var_ext_decls stmt_list '}'
   22              | '{' stmt_list '}'
   23              | '{' '}'

   24 function_decl: func_proto compound_stmt

   25 func_proto: func_hdr
   26           | ATOMIC func_hdr

   27 func_hdr: untyped_func_hdr
   28         | typed_func_hdr

   29 untyped_func_hdr: function_id formal_params

   30 typed_func_hdr: VOID function_id formal_params
   31               | var_typespec function_id formal_params

   32 function_id: IDENTIFIER

   33 formal_params: '(' param_list ')'

   34 param_list: %empty
   35           | VOID
   36           | param_groups

   37 param_groups: param_group
   38             | param_groups ',' param_group

   39 param_group: param_typespec newident

   40 param_typespec: var_typespec
   41               | var_typespec '&'
   42               | STRING

   43 monitor_decl: monitor_hdr '{' mon_internal_decls '}'

   44 monitor_hdr: MONITOR newident

   45 mon_internal_decls: internal_decls monitor_init

   46 internal_decls: internal_decl
   47               | internal_decls internal_decl

   48 internal_decl: function_decl
   49              | const_decl
   50              | var_decl

   51 monitor_init: %empty
   52             | the_init '{' stmt_list '}'

   53 the_init: INIT

   54 extern_mon_spec: monitor_hdr '{' mon_func_protos '}'

   55 mon_func_protos: func_proto ';'
   56                | mon_func_protos func_proto ';'

   57 var_ext_decls: var_ext_decl
   58              | var_ext_decls var_ext_decl

   59 var_ext_decl: var_decl
   60             | extern_decl

   61 var_decl: var_typespec var_list ';'

   62 var_typespec: INT
   63             | CHAR
   64             | STRING '[' unsigned_const ']'
   65             | ident

   66 var_list: var_name
   67         | var_list ',' var_name

   68 var_name: newident
   69         | newident '=' constant
   70         | array_decl

   71 array_decl: newident index_list

   72 index_list: index
   73           | index_list index

   74 index: '[' unsigned_const ']'

   75 type_decl: the_typedef var_typespec newident ';'
   76          | the_typedef var_typespec array_decl ';'

   77 the_typedef: TYPEDEF

   78 const_decl: CONST var_typespec newident '=' constant ';'

   79 constant: unsigned_const
   80         | '-' unsigned_const
   81         | '+' unsigned_const
   82         | CHARCON

   83 unsigned_const: unsigned_num
   84               | ident

   85 unsigned_num: UNSIGNED_INT

   86 extern_decl: extern_func_decl
   87            | extern_var_decl
   88            | extern_array_decl
   89            | extern_mon_decl

   90 extern_func_decl: the_extern func_proto ';'

   91 the_extern: EXTERN

   92 extern_var_decl: the_extern var_decl

   93 extern_array_decl: the_extern array_decl

   94 extern_mon_decl: the_extern extern_mon_spec

   95 stmt_list: stmt
   96          | stmt_list stmt
   97          | error ';'
   98          | error '}'

   99 opt_for_expr_list: %empty
  100                  | for_expr_list

  101 for_expr_list: expr
  102              | for_expr_list ',' expr

  103 stmt: selection_stmt
  104     | compound_stmt
  105     | cobegin_stmt
  106     | iteration_stmt
  107     | expr_stmt
  108     | continue_stmt
  109     | break_stmt
  110     | return_stmt
  111     | output_stmt
  112     | input_stmt
  113     | null_stmt
  114     | special_proc_calls

  115 special_proc_calls: send_call
  116                   | broadcast_call
  117                   | sprintf_call

  118 send_call: SEND left_exprparm ',' expr ')' ';'

  119 left_exprparm: '(' rawstring_parm
  120              | '(' expr

  121 broadcast_call: BROADCAST single_exprparm

  122 sprintf_call: sprintf_buf_fmt ',' sprintf_parmlist ')'

  123 sprintf_buf_fmt: SPRINTF '(' expr ',' rawstring_parm

  124 sprintf_parmlist: expr
  125                 | sprintf_parmlist ',' expr

  126 selection_stmt: if_expr then_stmt
  127               | if_expr then_stmt the_else stmt
  128               | switch_hdr '{' case_groups opt_default '}'

  129 if_expr: IF '(' expr ')'

  130 then_stmt: stmt

  131 the_else: ELSE

  132 switch_hdr: SWITCH '(' expr ')'

  133 case_groups: case_group
  134            | case_groups case_group

  135 case_group: case_hdr_list patch_case_hdrs stmt_list

  136 case_hdr_list: case_hdr
  137              | case_hdr_list case_hdr

  138 case_hdr: CASE begin_case expr ':'

  139 begin_case: %empty

  140 patch_case_hdrs: %empty

  141 opt_default: %empty
  142            | the_default stmt_list

  143 the_default: DEFAULT ':'

  144 cobegin_stmt: cbegin '{' stmt_list '}'

  145 iteration_stmt: the_while while_expr stmt
  146               | the_do do_stmt_loc stmt the_do_while do_while_expr ';'
  147               | for_init opt_continue_expr for_semic2 opt_iter_exprs ')' stmt

  148 the_while: WHILE

  149 while_expr: '(' expr ')'

  150 the_do: DO

  151 do_stmt_loc: %empty

  152 the_do_while: WHILE

  153 do_while_expr: '(' expr ')'

  154 for_init: the_for opt_for_expr_list ';'

  155 the_for: FOR '('

  156 opt_continue_expr: %empty
  157                  | expr

  158 for_semic2: ';'

  159 opt_iter_exprs: %empty
  160               | for_expr_list

  161 cbegin: CBEGIN

  162 continue_stmt: CONTINUE ';'

  163 break_stmt: BREAK ';'

  164 return_stmt: the_return opt_expr ';'

  165 the_return: RETURN

  166 output_stmt: COUT output_list ';'

  167 output_list: LTLT output_item
  168            | output_list LTLT output_item

  169 output_item: expr
  170            | ENDL
  171            | RAWSTRING

  172 input_stmt: CIN GTGT expr ';'

  173 null_stmt: ';'

  174 opt_expr: %empty
  175         | expr

  176 expr_stmt: expr ';'

  177 expr: logical_or_expr
  178     | var_assign

  179 var_assign: id_becomes expr
  180           | array_var_becomes expr

  181 id_becomes: ident '='

  182 array_var_becomes: array_var '='

  183 logical_or_expr: logical_and_expr
  184                | left_logical_or_expr OR logical_and_expr

  185 left_logical_or_expr: logical_or_expr

  186 logical_and_expr: relational_expr
  187                 | left_logical_and_expr AND relational_expr

  188 left_logical_and_expr: logical_and_expr

  189 relational_expr: simple_expr
  190                | left_relational_expr relational_op simple_expr

  191 left_relational_expr: simple_expr

  192 relational_op: EQ
  193              | '<'
  194              | '>'
  195              | LE
  196              | GE
  197              | NE

  198 simple_expr: term
  199            | '+' term
  200            | '-' term
  201            | left_simple_expr add_op term

  202 left_simple_expr: simple_expr

  203 add_op: '+'
  204       | '-'

  205 term: factor
  206     | left_term mult_op factor

  207 left_term: term

  208 mult_op: '*'
  209        | '/'
  210        | '%'

  211 factor: variable
  212       | unsigned_num
  213       | CHARCON
  214       | '(' expr ')'
  215       | '!' factor

  216 variable: var_ident
  217         | var_ident PLPL
  218         | PLPL var_ident
  219         | var_ident MIMI
  220         | MIMI var_ident
  221         | func_call
  222         | array_var
  223         | typecast

  224 var_ident: ident

  225 func_call: func_id actual_params
  226          | stringfn_call
  227          | receive_call

  228 func_id: ident

  229 actual_params: '(' ')'
  230              | '(' actuals_list ')'

  231 actuals_list: actuals_list ',' actual_param
  232             | actual_param

  233 actual_param: expr

  234 stringfn_call: stringconcat_call
  235              | stringcopy_call
  236              | stringcompare_call
  237              | stringlength_call
  238              | sscanf_call

  239 stringconcat_call: STRINGCONCAT left_exprparm right_stringparm

  240 right_stringparm: ',' expr ')'
  241                 | ',' rawstring_parm ')'

  242 rawstring_parm: RAWSTRING

  243 stringcopy_call: STRINGCOPY left_exprparm right_stringparm

  244 stringcompare_call: STRINGCOMPARE left_exprparm right_stringparm

  245 stringlength_call: STRINGLENGTH single_exprparm

  246 single_exprparm: '(' expr ')'
  247                | '(' RAWSTRING ')'

  248 sscanf_call: sscanf_buf_fmt ',' sscanf_parmlist ')'

  249 sscanf_buf_fmt: SSCANF '(' expr ',' rawstring_parm

  250 sscanf_parmlist: expr
  251                | sscanf_parmlist ',' expr

  252 receive_call: RECEIVE left_exprparm ')'
  253             | RECEIVE left_exprparm ',' expr ')'

  254 array_var: array_id index_expr

  255 array_id: ident

  256 index_expr: index_expr '[' expr ']'
  257           | '[' expr ']'

  258 typecast: INT '(' expr ')'

  259 ident: IDENTIFIER

  260 newident: IDENTIFIER


